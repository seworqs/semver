{
    "name": "seworqs/semver",
    "description": "A type-safe, chainable semantic versioning implementation for PHP with support for bumping versions, handling pre-releases, and comparing versions.",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": [
        "semver", "semantic versioning", "version", "bump", "pre-release",
        "php", "versioning", "compare", "release", "seworqs"
    ],
    "version": "1.0.3",
    "authors": [
        {
            "name": "S.E. Franke",
            "email": "sven.franke@seworqs.eu"
        }
    ],
    "autoload": {
        "psr-4": {
            "Seworqs\\Semver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Seworqs\\Semver\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11"
    },
    "scripts": {
        "test": "phpunit"
    }
}
